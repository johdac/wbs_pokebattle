meta {
  name: login
  type: http
  seq: 1
}

post {
  url: {{authUrl}}/auth/login
  body: json
  auth: inherit
}

body:json {
  {
    "email": "{{email}}",
    "password": "{{userPassword}}"
  }
}

vars:post-response {
  refreshToken: res.body.refreshToken
  accessToken: res.body.accessToken
}

settings {
  encodeUrl: true
  timeout: 0
}
